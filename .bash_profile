# Nathan Hartman's bash profile

# Misc
alias vbp='vi ~/.bash_profile'
alias sbp='source ~/.bash_profile'

# Git
alias gs='git status'
alias gd='git diff'
alias gcm='git commit -m'
alias gl='git log'
alias gf='git fetch'
alias gp='git pull'
alias ga='git add'

# CLI
alias ll='ls -l --color=auto'
alias grep='grep --color=auto'
alias mkdir='mkdir -pv'
alias e='emacs'
alias emacs='emacs -nw'

# k8s
alias k='kubectl'

